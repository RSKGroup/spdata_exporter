<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.rskgroup.spdata_exporter</string>
    <key>ProgramArguments</key>
	<array>
		 <string>/usr/local/bin/spdata_exporter</string>
		 <string>--config</string>
		 <string>/usr/local/etc/spdata_exporter.yml</string>
	</array> 
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<bool>true</bool>      
</dict>
</plist>